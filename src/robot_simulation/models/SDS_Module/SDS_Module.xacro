<?xml version='1.0'?>
<sdf version='1.7' xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:macro name="SDL_Module" params="suffix:='' parent_link:='' x:=0 y:=0 z:=0 roll:=0 pitch:=0 yaw:=0">
        <xacro:if value="${suffix == ''}">
            <xacro:property name="model_name" value="SDS_Module" />
        </xacro:if>
        <xacro:unless value="${suffix == ''}">
            <xacro:property name="model_name" value="SDS_Module_${suffix}" />
        </xacro:unless>

        <link name='${model_name}_Fixed'>
            <inertial>
                <mass>2.268</mass>
                <inertia>
                    <ixx>0.00760197</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.00779099</iyy>
                    <iyz>0</iyz>
                    <izz>0.01281875</izz>
                </inertia>
                <pose>0 0 0 0 -0 0</pose>
            </inertial>
            <pose>${x + 6.9e-05} ${y} ${z + 0.008921} ${roll} ${pitch} ${yaw}</pose>
            <gravity>1</gravity>
            <self_collide>0</self_collide>
            <kinematic>0</kinematic>
            <enable_wind>0</enable_wind>
            <visual name='visual'>
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <uri>$(find robot_simulation)/models/SDS_Module/SDS_Fixed.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
                <transparency>0</transparency>
                <cast_shadows>1</cast_shadows>
            </visual>
            <collision name='collision'>
                <laser_retro>0</laser_retro>
                <max_contacts>10</max_contacts>
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <uri>$(find robot_simulation)/models/SDS_Module/SDS_Fixed_Collision.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1</mu>
                            <mu2>1</mu2>
                            <fdir1>0 0 0</fdir1>
                            <slip1>0</slip1>
                            <slip2>0</slip2>
                        </ode>
                        <torsional>
                            <coefficient>1</coefficient>
                            <patch_radius>0</patch_radius>
                            <surface_radius>0</surface_radius>
                            <use_patch_radius>1</use_patch_radius>
                            <ode>
                                <slip>0</slip>
                            </ode>
                        </torsional>
                    </friction>
                    <bounce>
                        <restitution_coefficient>0</restitution_coefficient>
                        <threshold>1e+06</threshold>
                    </bounce>
                    <contact>
                        <collide_without_contact>0</collide_without_contact>
                        <collide_without_contact_bitmask>1</collide_without_contact_bitmask>
                        <collide_bitmask>1</collide_bitmask>
                        <ode>
                            <soft_cfm>0</soft_cfm>
                            <soft_erp>0.2</soft_erp>
                            <kp>1e+13</kp>
                            <kd>1</kd>
                            <max_vel>0.01</max_vel>
                            <min_depth>0</min_depth>
                        </ode>
                        <bullet>
                            <split_impulse>1</split_impulse>
                            <split_impulse_penetration_threshold>-0.01</split_impulse_penetration_threshold>
                            <soft_cfm>0</soft_cfm>
                            <soft_erp>0.2</soft_erp>
                            <kp>1e+13</kp>
                            <kd>1</kd>
                        </bullet>
                    </contact>
                </surface>
            </collision>
        </link>
        <link name='${model_name}_Rotation'>
            <inertial>
                <mass>0.59</mass>
                <inertia>
                    <ixx>0.0006558</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.00065583</iyy>
                    <iyz>0</iyz>
                    <izz>0.00074316</izz>
                </inertia>
                <pose>0 0 0 0 -0 0</pose>
            </inertial>
            <pose>${x + 6.9e-05} ${y} ${z + 0.026037} ${roll} ${pitch} ${yaw}</pose>
            <gravity>1</gravity>
            <self_collide>0</self_collide>
            <kinematic>0</kinematic>
            <enable_wind>0</enable_wind>
            <visual name='visual'>
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <uri>$(find robot_simulation)/models/SDS_Module/SDS_Rotation.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
                <material>
                    <lighting>1</lighting>
                    <script>
                        <uri>file://media/materials/scripts/gazebo.material</uri>
                        <name>Gazebo/Grey</name>
                    </script>
                    <shader type='pixel'>
                        <normal_map>__default__</normal_map>
                    </shader>
                    <ambient>0.3 0.3 0.3 1</ambient>
                    <diffuse>0.7 0.7 0.7 1</diffuse>
                    <specular>0.01 0.01 0.01 1</specular>
                    <emissive>0 0 0 1</emissive>
                </material>
                <transparency>0</transparency>
                <cast_shadows>1</cast_shadows>
            </visual>
            <collision name='collision'>
                <laser_retro>0</laser_retro>
                <max_contacts>10</max_contacts>
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <uri>$(find robot_simulation)/models/SDS_Module/SDS_Rotation_Collision.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1</mu>
                            <mu2>1</mu2>
                            <fdir1>0 0 0</fdir1>
                            <slip1>0</slip1>
                            <slip2>0</slip2>
                        </ode>
                        <torsional>
                            <coefficient>1</coefficient>
                            <patch_radius>0</patch_radius>
                            <surface_radius>0</surface_radius>
                            <use_patch_radius>1</use_patch_radius>
                            <ode>
                                <slip>0</slip>
                            </ode>
                        </torsional>
                    </friction>
                    <bounce>
                        <restitution_coefficient>0</restitution_coefficient>
                        <threshold>1e+06</threshold>
                    </bounce>
                    <contact>
                        <collide_without_contact>0</collide_without_contact>
                        <collide_without_contact_bitmask>1</collide_without_contact_bitmask>
                        <collide_bitmask>1</collide_bitmask>
                        <ode>
                            <soft_cfm>0</soft_cfm>
                            <soft_erp>0.2</soft_erp>
                            <kp>1e+13</kp>
                            <kd>1</kd>
                            <max_vel>0.01</max_vel>
                            <min_depth>0</min_depth>
                        </ode>
                        <bullet>
                            <split_impulse>1</split_impulse>
                            <split_impulse_penetration_threshold>-0.01</split_impulse_penetration_threshold>
                            <soft_cfm>0</soft_cfm>
                            <soft_erp>0.2</soft_erp>
                            <kp>1e+13</kp>
                            <kd>1</kd>
                        </bullet>
                    </contact>
                </surface>
            </collision>
        </link>
        <link name='${model_name}_Wheel'>
            <inertial>
                <mass>0.24</mass>
                <inertia>
                    <ixx>0.00025496</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.00025496</iyy>
                    <iyz>0</iyz>
                    <izz>0.00018653</izz>
                </inertia>
                <pose>0 0 0 0 -0 0</pose>
            </inertial>
            <pose>${x -0.000137} ${y} ${z -0.034957} ${roll} ${pitch} ${yaw}</pose>
            <gravity>1</gravity>
            <self_collide>0</self_collide>
            <kinematic>0</kinematic>
            <enable_wind>0</enable_wind>
            <visual name='visual'>
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <uri>$(find robot_simulation)/models/SDS_Module/SDS_Wheel.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
                <transparency>0</transparency>
                <cast_shadows>1</cast_shadows>
            </visual>
            <collision name='collision'>
                <laser_retro>0</laser_retro>
                <max_contacts>10</max_contacts>
                <pose>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <uri>$(find robot_simulation)/models/SDS_Module/SDS_Wheel_Collision.dae</uri>
                        <scale>1 1 1</scale>
                    </mesh>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1</mu>
                            <mu2>1</mu2>
                            <fdir1>0 0 0</fdir1>
                            <slip1>0</slip1>
                            <slip2>0</slip2>
                        </ode>
                        <torsional>
                            <coefficient>1</coefficient>
                            <patch_radius>0</patch_radius>
                            <surface_radius>0</surface_radius>
                            <use_patch_radius>1</use_patch_radius>
                            <ode>
                                <slip>0</slip>
                            </ode>
                        </torsional>
                    </friction>
                    <bounce>
                        <restitution_coefficient>0</restitution_coefficient>
                        <threshold>1e+06</threshold>
                    </bounce>
                    <contact>
                        <collide_without_contact>0</collide_without_contact>
                        <collide_without_contact_bitmask>1</collide_without_contact_bitmask>
                        <collide_bitmask>1</collide_bitmask>
                        <ode>
                            <soft_cfm>0</soft_cfm>
                            <soft_erp>0.2</soft_erp>
                            <kp>1e+13</kp>
                            <kd>1</kd>
                            <max_vel>0.01</max_vel>
                            <min_depth>0</min_depth>
                        </ode>
                        <bullet>
                            <split_impulse>1</split_impulse>
                            <split_impulse_penetration_threshold>-0.01</split_impulse_penetration_threshold>
                            <soft_cfm>0</soft_cfm>
                            <soft_erp>0.2</soft_erp>
                            <kp>1e+13</kp>
                            <kd>1</kd>
                        </bullet>
                    </contact>
                </surface>
            </collision>
        </link>

        <joint name='${model_name}_rotation_joint' type='revolute'>
            <parent>${model_name}_Fixed</parent>
            <child>${model_name}_Rotation</child>
            <pose>0 0 0 0 -0 0</pose>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                    <effort>-1</effort>
                    <velocity>-1</velocity>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                    <damping>0.17</damping>
                    <friction>0.0</friction>
                </dynamics>
            </axis>
            <physics>
                <ode>
                    <limit>
                        <cfm>0</cfm>
                        <erp>0.2</erp>
                    </limit>
                    <suspension>
                        <cfm>0</cfm>
                        <erp>0.2</erp>
                    </suspension>
                </ode>
            </physics>
        </joint>
        <joint name='${model_name}_wheel_joint' type='revolute'>
            <parent>${model_name}_Rotation</parent>
            <child>${model_name}_Wheel</child>
            <pose>0 0 0 0 -0 0</pose>
            <axis>
                <xyz>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                    <effort>-1</effort>
                    <velocity>-1</velocity>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                    <damping>0.04</damping>
                    <friction>0.0</friction>
                </dynamics>
            </axis>
            <physics>
                <ode>
                    <limit>
                        <cfm>0.0</cfm>
                        <erp>0.2</erp>
                    </limit>
                    <suspension>
                        <cfm>0</cfm>
                        <erp>0.2</erp>
                    </suspension>
                </ode>
            </physics>
        </joint>

        <xacro:unless value="${parent_link == ''}">
            <joint name='${model_name}_to_${parent_link}_fixed_joint' type='fixed'>
                <parent>${parent_link}</parent>
                <child>${model_name}_Fixed</child>
            </joint>
        </xacro:unless>


        <plugin name='NTMotor_${model_name}_Rotation' filename='libnt_motor_plugin.so'>
            <joint_name>${model_name}_rotation_joint</joint_name>
            <joint_axis>0</joint_axis>
            <torque_axis>z</torque_axis>
            <motor_model>Falcon</motor_model>
            <gear_ratio>21.4285714286</gear_ratio>
        </plugin>

        <plugin name='NTMotor_${model_name}_Wheel' filename='libnt_motor_plugin.so'>
            <joint_name>${model_name}_wheel_joint</joint_name>
            <joint_axis>0</joint_axis>
            <torque_axis>y</torque_axis>
            <motor_model>Falcon</motor_model>
            <gear_ratio>5.9027777</gear_ratio>
        </plugin>

        <plugin name='NTMotor_${model_name}_Rotation_CANCoder' filename='libnt_cancoder_plugin.so'>
            <joint_name>${model_name}_rotation_joint</joint_name>
        </plugin>
    </xacro:macro>
</sdf>